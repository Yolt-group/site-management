VALUES(80c051a0-2cf8-45ec-bfe3-c884b617cac0, '{ "explanationField": { "id": "EXPLANATION", "displayName": "Yolt supports login with your Username, Password and your full memorable information��� please note these are case sensitive.", "explanation": "<!DOCTYPE html><html lang=\"en\"><head><title>Halifax</title><meta charset=\"utf-8\"><meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\"><style> html, body { margin: 0; padding: 0; width: 100%; background-color: #F2F4F8; font-size: 14px; line-height: 24px; } html { height: 100%; background-color: #F2F4F8; } body { height: auto; font-family: \"GothamSSm-Book\"; color: #2D2D30; width: calc(100%); font-weight: 400; font-size: 14px; } h3 { color: #2d2d30; font-size: 14px; line-height: 24px; font-family: \"GothamSSm-Book\"; font-weight: 500; margin: 24px; text-align: center; } section { background: white; width: calc(100% - 48px); padding: 24px; border-top: 0.5px solid rgba(242, 244, 248, 1); border-bottom: 0.5px solid rgba(242, 244, 248, 1); height: auto; margin-bottom: 40px; box-shadow: 0 4px 8px 0 rgba(36, 59, 111, 0.15); } section h1 { color: #033072; padding: 0px; margin: 0px; margin-bottom: 8px; font-size: 16px; font-family: \"GothamRndSSm-Medium\"; font-weight: 500; } section p { margin: 0px; font-size: 14px; margin-bottom: 4px; } </style></head><body><section><h1>Adding your account to Yolt</h1><p>To add your Halifax account to Yolt, you&apos;ll need to enter the same log in information you use for the Halifax Online Banking website. This includes your username, password and your full memorable information.</b></p><br><ul><li>Your username was given to you when you registered for Halifax Online Banking</li><li>The &lsquo;password&rsquo; needed to add your Halifax account to Yolt is the same one you use to Log in to the Halifax Online Banking Portal, this is usually a series of letters and numbers</li><li>Your &lsquo;memorable information&rsquo; is usually 6&#45;15 characters longs, with no spaces. It contains at least 3 letters and 1 number and is always different to your Username and Password</li></ul><br></section><br><br></body></html>"}, "formComponents": ["java.util.ArrayList", [["nl.ing.lovebird.sitemanagement.service.domain.TextField", { "id": "LOGIN", "displayName": "Username", "length": 20, "maxLength": 30, "optional": false } ], ["nl.ing.lovebird.sitemanagement.service.domain.PasswordField", { "id": "PASSWORD", "displayName": "Password", "length": 20, "maxLength": 20, "optional": false } ], ["nl.ing.lovebird.sitemanagement.service.domain.PasswordField", { "id": "PASSWORD1", "displayName": "Memorable Information", "length": 20, "maxLength": 40, "optional": false } ]]] }', '{"conjunctionOp":{"conjuctionOp":1},"componentList":[{"valueIdentifier":"LOGIN","valueMask":"LOGIN_FIELD","fieldType":{"typeName":"IF_LOGIN"},"size":20,"maxlength":30,"fieldInfoType":"com.yodlee.common.FieldInfoSingle","name":"LOGIN","displayName":"Username","isEditable":true,"isOptional":false,"isEscaped":false,"helpText":"133161","isOptionalMFA":false,"isMFA":false},{"valueIdentifier":"PASSWORD","valueMask":"LOGIN_FIELD","fieldType":{"typeName":"IF_PASSWORD"},"size":20,"maxlength":20,"fieldInfoType":"com.yodlee.common.FieldInfoSingle","name":"PASSWORD","displayName":"Password","isEditable":true,"isOptional":false,"isEscaped":false,"helpText":"133160","isOptionalMFA":false,"isMFA":false},{"valueIdentifier":"PASSWORD1","valueMask":"LOGIN_FIELD","fieldType":{"typeName":"IF_PASSWORD"},"size":20,"maxlength":40,"fieldInfoType":"com.yodlee.common.FieldInfoSingle","name":"PASSWORD1","displayName":"Memorable Information","isEditable":true,"isOptional":false,"isEscaped":false,"helpText":"133155","isOptionalMFA":false,"isMFA":false}],"defaultHelpText":"3129"}');
